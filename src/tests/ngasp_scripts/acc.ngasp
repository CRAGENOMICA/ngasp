verbose --level normal

dim -n x --as int
dim -n v --as int_vector

dim -n m_x --as int_matrix
dim -n m_v --as int_matrix

set-value --to x --eq 1
set-value --to v --eq "1,2,3,4,5,6,7,8,9"


dim -n ite --as int
set-value --to ite --eq 0
for --ite ite --from 0 --to 3 --inc 1

  dim -n vaux --as int_vector
  set-value --to vaux --eq "0,0"
  set-value --to vaux[0] --eq ite
  set-value --to vaux[1] --eq x
  set-value --to m_v --eq vaux
  print -n vaux --mode value --eol --text "vaux : "
  print -n m_v --mode value --eol --text "m_v  : "
  del -n vaux

 set-value --to m_x --eq x --index ite
 set-value --to m_v[0] --eq v

 print -n ite --mode value --eol --text "ite: "
 print -n x --mode value --eol --text "x: "
 print -n v --mode value --eol --text "v: "
 print -n m_x --mode value --eol --text "m_x: "
 print -n m_v --mode value --eol --text "m_v: "

end-for


